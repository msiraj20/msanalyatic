{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "trainingsirajadf"
		},
		"BlobMetadataSQLConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobMetadataSQLConnection'"
		},
		"DLSTargetProviderFunction_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'DLSTargetProviderFunction'"
		},
		"OrganizedDataLakeConnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'OrganizedDataLakeConnection'"
		},
		"OrdersBlobStorageConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OrdersBlobStorageConnection'"
		},
		"DLSTargetProviderFunction_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://traniningfunctionappsiraj.azurewebsites.net"
		},
		"TrainingAzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://trainingsirajkv.vault.azure.net/"
		},
		"OrganizedDataLakeConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://traniningsirajdls.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "trainingsirajadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"BlobMetadataSQLConnection_connectionString": {
						"value": "[parameters('BlobMetadataSQLConnection_connectionString')]"
					},
					"DLSTargetProviderFunction_functionKey": {
						"value": "[parameters('DLSTargetProviderFunction_functionKey')]"
					},
					"OrganizedDataLakeConnection_accountKey": {
						"value": "[parameters('OrganizedDataLakeConnection_accountKey')]"
					},
					"OrdersBlobStorageConnection_connectionString": {
						"value": "[parameters('OrdersBlobStorageConnection_connectionString')]"
					},
					"DLSTargetProviderFunction_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('DLSTargetProviderFunction_properties_typeProperties_functionAppUrl')]"
					},
					"TrainingAzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('TrainingAzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"OrganizedDataLakeConnection_properties_typeProperties_url": {
						"value": "[parameters('OrganizedDataLakeConnection_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}